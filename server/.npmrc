; Comments start with a semicolon (;)

; Set the registry URL
registry=https://registry.npmjs.org/

; Set the cache directory
cache=~/.npm/cache

; Save exact versions of dependencies by default
save-exact=true

; Always save dependencies to `package.json`
save=true

; Specify a custom prefix for globally installed packages
prefix=/usr/local

; Set the log level
loglevel=warn

